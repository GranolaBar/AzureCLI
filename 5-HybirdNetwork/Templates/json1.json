{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "publicIPAddresses_gwpip_name": {"defaultValue": "gwpip","type": "String" },
    "virtualNetworks_azureVnet_name": {"defaultValue": "azureVnet","type": "String" } },
  
  "variables": { },
  "resources": [
    {
      
      "type": "Microsoft.Network/publicIPAddresses",
      "name": "[parameters('publicIPAddresses_gwpip_name')]",
      "apiVersion": "2016-03-30",
      "location": "centralus",
      "properties": {
        "publicIPAllocationMethod": "Dynamic",
        "idleTimeoutInMinutes": 4
      },
      "dependsOn": [ ]
    },
    {
      "comments": "Generalized from resource: '/subscriptions/c429c32e-5f31-4b5a-bb06-a21f12ce07b4/resourceGroups/S2SExport/providers/Microsoft.Network/virtualNetworks/azureVnet'.",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[parameters('virtualNetworks_azureVnet_name')]",
      "apiVersion": "2016-03-30",
      "location": "centralus",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "10.1.0.0/16"
          ]
        },
        "subnets": [
          {
            "name": "Subnet1",
            "properties": {
              "addressPrefix": "10.1.0.0/24"
            }
          },
          {
            "name": "GatewaySubnet",
            "properties": {
              "addressPrefix": "10.1.1.0/24"
            }
          },
          {
            "name": "Subnet2",
            "properties": {
              "addressPrefix": "10.1.2.0/24"
            }
          }
        ]
      },
      "dependsOn": [ ]
    }
  ]
}
